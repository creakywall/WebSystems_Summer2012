<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<form id="addClassForm">
			<fieldset>
			<span>Fill out the form below. Every field is required.</span>
				<div>
					<label>Email</label>
					<g:TextBox ui:field="emailBox"/>
				</div>
				<div>
		    		<label>Subject</label>
		    		<g:TextBox ui:field="subjectBox"/>
	    		</div>
	    		<div>
		    		<label>Term</label>
		    		<g:TextBox ui:field="termBox"/>
	    		</div>
	    		<div>
		    		<label>Catalog Number</label>
		    		<g:TextBox ui:field="catalogBox"/>
	    		</div>
	    		<div>
		    		<label>Section</label>
		    		<g:TextBox ui:field="sectionBox"/>
		    	</div>
    		</fieldset>
    	</form>
    	<g:Button ui:field="submitClass"/>
	</g:HTMLPanel>
</ui:UiBinder> 