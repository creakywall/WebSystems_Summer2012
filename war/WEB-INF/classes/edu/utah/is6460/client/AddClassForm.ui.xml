<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<form id="addClassForm">
    		<label>Subject</label>
    		<g:TextBox ui:field="subjectBox"/>
    		<label>Term</label>
    		<g:TextBox ui:field="termBox"/>
    		<label>Catalog</label>
    		<g:TextBox ui:field="catalogBox"/>
    	</form>
    	<g:Button ui:field="submitClass"/>
	</g:HTMLPanel>
</ui:UiBinder> 